// Leetcode 268 - Missing Number
// Approach: Calculate expected sum of numbers from 0 to n and subtract array sum
// Time Complexity: O(n)
// Space Complexity: O(1)

class Solution {
    public int missingNumber(int[] nums) {
        int n = nums.length;
        int ts = n*(n+1)/2;
        int numssum=0;
        for(int i=0; i<n; i++){
            numssum += nums[i];
        }
        return ts-numssum;
        
    }
}
